<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lifebyte.hubx.admin.mapper.InstrumentMapper">

    <select id="queryAllCurrencies" resultType="com.lifebyte.hubx.data.entity.account.TradingCurrencyVO">
        SELECT c.id,
               c.currency
        FROM trading_currency c
    </select>
    <select id="queryAllLeverages" resultType="com.lifebyte.hubx.data.entity.account.TradingLeverageVO">
        SELECT l.id,
               l.leverage
        FROM trading_leverage l
    </select>

</mapper>